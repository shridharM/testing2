                    };

            }
        }

        var getXType = function(fieldType) {
            if(fieldType == 'DATE') {
                return 'datecolumn'; //{xtype: 'datecolumn'}  Ext.util.Format.dateRenderer('M d, Y');
            } else {
                return '';
