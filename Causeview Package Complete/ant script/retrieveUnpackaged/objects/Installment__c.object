        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISBLANK( Pledge__r.Organization__c ), HYPERLINK(&quot;/&quot; &amp; Pledge__r.Constituent__c, Pledge__r.Constituent__r.FirstName &amp; &quot; &quot; &amp; Pledge__r.Constituent__r.LastName,&quot;_self&quot;), HYPERLINK(&quot;/&quot; &amp; Pledge__r.Organization__c, Pledge__r.Organization__r.Name,&quot;_self&quot;))</formula>
