    <fields>
        <fullName>Formatted_Payment_Date__c</fullName>
        <deprecated>false</deprecated>
        <description>Formatted payment date used for receipt merging</description>
        <externalId>false</externalId>
        <formula>CASE(MONTH(Date__c),1, &quot;January&quot;,
2, &quot;February&quot;,
