    <fields>
        <fullName>Days_Overdue__c</fullName>
        <deprecated>false</deprecated>
        <description>Calculates number of days payment is overdue</description>
        <externalId>false</externalId>
        <formula>IF (AND(ISPICKVAL( Status__c , &quot;Pending&quot;),Date__c &lt; Today ()),  Today() - Date__c, 
IF (AND(ISPICKVAL( Status__c , &quot;Committed&quot;),Date__c &lt; Today ()),  Today() - Date__c,0))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Calculates number of days payment is overdue</inlineHelpText>
        <label>Days Overdue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
