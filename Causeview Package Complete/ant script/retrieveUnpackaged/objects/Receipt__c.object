receipt_new.cv_pkg_dev_I__Duplicate_Receipt__c = receipt_master.Name + &apos;-&apos; + parseInt(receipt_master.cv_pkg_dev_I__Receipt_Count__c);
receipt_new.RecordTypeId = record.Id;
receipt_new.cv_pkg_dev_I__Original_Receipt__c = parent.location.href.substr(parent.location.href.lastIndexOf(&quot;/&quot;)+1, 15);
result = sforce.connection.create([receipt_new]);
var receipt_old = new sforce.SObject(&quot;cv_pkg_dev_I__Receipt__c&quot;);
receipt_old.id = parent.location.href.substr(parent.location.href.lastIndexOf(&quot;/&quot;)+1, 15);
receipt_old.cv_pkg_dev_I__Receipt_Count__c = parseInt(receipt_master.cv_pkg_dev_I__Receipt_Count__c) + 1;
result = sforce.connection.update([receipt_old]);
window.location.reload();</url>
