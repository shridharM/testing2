        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
result = sforce.connection.query(&quot;Select g.cv_pkg_dev_I__Receipt_Count__c, g.cv_pkg_dev_I__Amount_Receipted__c, g.cv_pkg_dev_I__Auto_Email_Receipt__c, g.cv_pkg_dev_I__Constituent__c, g.cv_pkg_dev_I__Date_Issued__c, g.cv_pkg_dev_I__Gift__c, g.cv_pkg_dev_I__Notes__c, g.cv_pkg_dev_I__Percent_Eligible__c, g.cv_pkg_dev_I__Print_Date__c, g.cv_pkg_dev_I__Receipt_Address__c, g.cv_pkg_dev_I__Receipt_Amount__c, g.cv_pkg_dev_I__Receipt_Date__c, g.cv_pkg_dev_I__Receipt_Email_Address__c, g.cv_pkg_dev_I__Receipt_Name__c, g.cv_pkg_dev_I__Status__c, g.Name, g.RecordTypeId from cv_pkg_dev_I__Receipt__c g Where id = &apos;&quot; + parent.location.href.substr(parent.location.href.lastIndexOf(&quot;/&quot;)+1, 15) + &quot;&apos;&quot;);
records = result.getArray(&quot;records&quot;);
var receipt_master = records[0];
var receipt_new = new sforce.SObject(&quot;cv_pkg_dev_I__Receipt__c&quot;);
result = sforce.connection.query(&quot;Select r.Id from RecordType r Where r.Name = &apos;Duplicate&apos; AND r.SobjectType = &apos;cv_pkg_dev_I__Receipt__c&apos;&quot;);
records = result.getArray(&quot;records&quot;);
receipt_new.cv_pkg_dev_I__Status__c = &quot;Duplicate&quot;;
var record = records[0];
receipt_new.cv_pkg_dev_I__Amount_Receipted__c = receipt_master.cv_pkg_dev_I__Amount_Receipted__c;
receipt_new.cv_pkg_dev_I__Auto_Email_Receipt__c = receipt_master.cv_pkg_dev_I__Auto_Email_Receipt__c;
receipt_new.cv_pkg_dev_I__Constituent__c = receipt_master.cv_pkg_dev_I__Constituent__c;
receipt_new.cv_pkg_dev_I__Date_Issued__c = receipt_master.cv_pkg_dev_I__Date_Issued__c;
receipt_new.cv_pkg_dev_I__Gift__c = receipt_master.cv_pkg_dev_I__Gift__c;
receipt_new.cv_pkg_dev_I__Notes__c = receipt_master.cv_pkg_dev_I__Notes__c;
receipt_new.cv_pkg_dev_I__Percent_Eligible__c = receipt_master.cv_pkg_dev_I__Percent_Eligible__c;
