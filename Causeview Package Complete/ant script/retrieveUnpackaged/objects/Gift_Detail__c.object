        <fullName>Approved_Amount__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Payment__c), 0, IF(OR(ISPICKVAL(Payment__r.Status__c, &quot;Declined&quot;), ISPICKVAL(Payment__r.Status__c, &quot;Pending&quot;), ISPICKVAL(Payment__r.Status__c, &quot;Written Off&quot;)), 0,
        IF(Payment__r.Amount__c = 0, 0, Amount__c)))</formula>
        <label>Approved Amount</label>
        <precision>18</precision>
        <required>false</required>
