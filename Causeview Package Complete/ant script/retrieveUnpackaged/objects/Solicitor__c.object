        <searchFilterFields>Gift__c</searchFilterFields>
        <searchFilterFields>Gift_Date__c</searchFilterFields>
        <searchFilterFields>Primary_Solicitor__c</searchFilterFields>
        <searchFilterFields>Role__c</searchFilterFields>
        <searchFilterFields>Solicitor__c</searchFilterFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>IndividualOrOrg</fullName>
        <active>true</active>
        <description>Validates that an soft credit is only linked to an individual or an organization, but not both.</description>
        <errorConditionFormula>NOT(ISBLANK( Solicitor__c )) &amp;&amp; NOT(ISBLANK(  Organization_Solicitor__c ))</errorConditionFormula>
        <errorMessage>Each soft credit record can only be credited to either an individual or an organization, but not both.  Delete your organization input and create an additional soft credit record for the organization after saving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SoftCreditAmount</fullName>
        <active>false</active>
        <description>Validates soft credit amount is not less than 0 and greater than transaction amount.</description>
        <errorConditionFormula>OR( Custom_Amount__c  &lt; 0, Custom_Amount__c &gt;  Gift__r.Amount__c)</errorConditionFormula>
        <errorDisplayField>Custom_Amount__c</errorDisplayField>
        <errorMessage>The soft credit amount cannot be less than 0 or exceed the Transaction approved amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SoftCreditAssignement</fullName>
        <active>false</active>
        <description>Validates organization and individual are not both blank.</description>
        <errorConditionFormula>AND(ISBLANK( Solicitor__c), ISBLANK( Organization_Solicitor__c ))</errorConditionFormula>
        <errorMessage>Both Organization Solicitor and Individual Solicitor cannot be blank.</errorMessage>
    </validationRules>
</CustomObject>
