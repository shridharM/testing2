    <fields>
        <fullName>Event_Level__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Event level purchased as part of Transaction</description>
        <externalId>false</externalId>
        <label>Event Level</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>The event level does not belong to the event you have allocated to.</errorMessage>
            <filterItems>
                <field>$Source.New_Campaign__c</field>
                <operation>equals</operation>
                <valueField>Event_Level__c.Event__c</valueField>
            </filterItems>
            <isOptional>false</isOptional>
