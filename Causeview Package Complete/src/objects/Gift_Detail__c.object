    <fields>
        <fullName>Amount__c</fullName>
        <externalId>false</externalId>
        <label>Allocated Amount</label>
        <precision>17</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Appeal_Name__c</fullName>
        <description>Name of appeal associated to gift.</description>
        <externalId>false</externalId>
        <formula>HYPERLINK(&quot;/&quot; &amp; Appeal__r.Id, Appeal__r.Appeal_Name__c, &quot;_self&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Appeal Name</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Appeal__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>No longer used by CauseView</description>
        <externalId>false</externalId>
        <label>(DEPRECATED)</label>
        <referenceTo>Appeal__c</referenceTo>
        <relationshipLabel>Allocations</relationshipLabel>
        <relationshipName>Split_Gifts</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Approved_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>IF( $Setup.App_Settings__c.Enable_Pledge_Accrual_Accounting__c &amp;&amp; ISPICKVAL(Gift__r.Gift_Type__c, &quot;Pledge&quot;),
Amount__c,
IF(ISBLANK(Payment__c), 
0, 
IF(OR(
ISPICKVAL(Payment__r.Status__c, &quot;Declined&quot;), 
ISPICKVAL(Payment__r.Status__c, &quot;Pending&quot;), 
ISPICKVAL(Payment__r.Status__c, &quot;Written Off&quot;), 
ISPICKVAL(Payment__r.Status__c, &quot;ACH Return&quot;)), 
0,
IF(Payment__r.Amount__c = 0, 
0, 
Amount__c)
)
)
)</formula>
        <label>Approved Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
