    <validationRules>
        <fullName>EventLevelAndQuantity</fullName>
        <active>true</active>
        <description>Enforces Quantity is entered with Event Level and vice versa.</description>
        <errorConditionFormula>IF(OR(AND(NOT(ISBLANK( Event_Level__c )), ISBLANK( Quantity__c )), (AND(ISBLANK( Event_Level__c ), NOT(ISBLANK( Quantity__c ))))), TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>You must enter both an Event Level and Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LockFund</fullName>
        <active>true</active>
        <description>Locks Fund value if Allocation has been posted to finance.</description>
        <errorConditionFormula>AND(ISCHANGED( Fund__c ), NOT(ISBLANK( Posted_to_Finance__c )))</errorConditionFormula>
        <errorDisplayField>Fund__c</errorDisplayField>
        <errorMessage>You cannot change the Fund since the Allocation has been posted to finance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Event_Level_Change</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISCHANGED( Event_Level__c ),ISCHANGED( Quantity__c ))</errorConditionFormula>
        <errorMessage>You cannot modify Event Level or Quantity.</errorMessage>
    </validationRules>
