        <fullName>Allocation_GA_Eligible__c</fullName>
        <externalId>false</externalId>
        <formula>If(Or(Is_Fund_GA_Eligible__c =false, Is_Payment_GA_Eligible__c=false),false,true)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
